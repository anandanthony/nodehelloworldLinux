{
  "name": "parse-server-azure-managed",
  "version": "1.7.0",
  "description": "Parse Server on Azure Managed Services",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/anandanthony/nodehelloworldLinux"
  },
  "license": "MIT",
  "dependencies": {
    "big.js": "^5.0.3",
    "body-parser": "^1.18.2",
    "buffer-shims": "^1.0.0",
    "cron": "^1.6.0",
    "currency-codes": "^1.5.0",
    "currency-symbol-map": "^4.0.4",
    "currency.js": "^1.2.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "exceljs": "^1.8.0",
    "express": "^4.13.4",
    "flow-remove-types": "^1.2.3",
    "http-status-codes": "^1.3.0",
    "invariant": "^2.2.4",
    "mixpanel": "^0.9.2",
    "moment-timezone": "^0.5.23",
    "multer": "^1.3.0",
    "node-fetch": "^2.1.1",
    "parse-dashboard": "^1.0.11",
    "parse-server": "*",
    "parse-server-azure-push": "^1.0.2",
    "parse-server-sendgrid-adapter": "^1.0.0",
    "twilio": "^3.19.2"
  },
  "scripts": {
    "build": "flow-remove-types src/ --out-dir build/ && cp -R src/public/. build/public",
    "flow": "flow",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "debug": "npm run build && nodemon -r dotenv/config -r flow-remove-types/register --inspect --watch src src/index.js",
    "lint": "eslint .",
    "test": "jest",
    "precommit": "lint-staged && npm run flow",
    "postinstall": "git update-index --skip-worktree local.js & exit 0"
  },
  "engines": {
    "node": "10.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "base-64": "^0.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^4.18.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "0.94.0",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.3",
    "jest-flow-transform": "^1.0.1",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.17.4",
    "prettier": "^1.12.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!parse)/"
    ],
    "transform": {
      "^.+\\.js(?:\\.flow)?$": "jest-flow-transform"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint"
  }
}
